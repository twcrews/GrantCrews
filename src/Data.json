        "Theme": {
            "typography": {
                "fontFamily": "'Noto Serif', 'Cambria', 'Georgia', 'Times New Roman', 'Times', 'Tahoma', serif"
            },
            "palette": {
                "primary": {
                    "light": "#6a23ff",
                    "main": "#8822ff",
                    "dark": "#6618bb",
                    "contrastText": "#fff"
                },
                "secondary": {
                    "main": "#eee"
                }
            }
        },
        "Styles": {
